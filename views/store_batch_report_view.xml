<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>
    <!-- Web Page Report Action -->
    <record id="action_store_batch_web_report" model="ir.actions.report">
        <field name="name">Store Batch Report</field>
        <field name="model">store.batch</field>
        <field name="groups_id" eval="[(4, ref('store_product_batch.group_store_batch_admin'))]"/>
        <field name="report_type">qweb-html</field>
        <field name="report_name">store_product_batch.report_store_batch_template</field>
        <field name="report_file">store_product_batch.report_store_batch_template</field>
        <field name="binding_type">report</field>
    </record>



    <record id="action_store_batch_web_report_prod" model="ir.actions.report">
        <field name="name">Store Batch Report</field>
        <field name="model">store.batch</field>
        <field name="groups_id" eval="[(4, ref('store_product_batch.group_store_batch_admin'))]"/>
        <field name="report_type">qweb-html</field>
        <field name="report_name">store_product_batch.report_store_batch_template_prod</field>
        <field name="report_file">store_product_batch.report_store_batch_template_prod</field>
        <field name="binding_type">report</field>
    </record>
  </data>
</odoo>

